<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="271.0" prefWidth="480.0"
            styleClass="roundedBordersDialog" stylesheets="@css/main.css"
            xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.dpsoftware.gui.controllers.ColorCorrectionDialogController">

    <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="271.0"
              prefWidth="480.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="60.0"/>
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="410.0"/>
        <ColumnConstraints hgrow="SOMETIMES" prefWidth="410.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="10.0"/>
        </padding>
        <Label text="%fxml.colorcorrection.red" wrapText="true"/>
        <Label text="%fxml.colorcorrection.yellow" wrapText="true" GridPane.rowIndex="1"/>
        <Label text="%fxml.colorcorrection.green" wrapText="true" GridPane.rowIndex="2"/>
        <Label text="%fxml.colorcorrection.cyan" wrapText="true" GridPane.rowIndex="3"/>
        <Label text="%fxml.colorcorrection.blue" wrapText="true" GridPane.rowIndex="4"/>
        <Label text="%fxml.colorcorrection.magenta" wrapText="true" GridPane.rowIndex="5"/>
        <Label text="%fxml.colorcorrection.saturation" wrapText="true" GridPane.rowIndex="6"/>

        <Slider fx:id="redSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="redSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Slider fx:id="yellowSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0"
                minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="yellowSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Slider fx:id="greenSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="greenSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Slider fx:id="cyanSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="cyanSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Slider fx:id="blueSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="blueSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Slider fx:id="magentaSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0"
                minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="magentaSaturation"
                GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <Slider fx:id="saturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                prefWidth="181.0" showTickMarks="true" snapToTicks="true" styleClass="saturation"
                GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#saveAndClose" text="%fxml.colorcorrection.ok"
                GridPane.rowIndex="7">
            <GridPane.margin>
                <Insets top="5.0"/>
            </GridPane.margin>
        </Button>
        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2"
              GridPane.halignment="RIGHT" GridPane.rowIndex="7">
            <padding>
                <Insets top="6.0"/>
            </padding>
            <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#close"
                    text="%fxml.colorcorrection.cancel">
                <HBox.margin>
                    <Insets/>
                </HBox.margin>
            </Button>
            <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#reset"
                    text="%fxml.colorcorrection.reset">
                <HBox.margin>
                    <Insets left="7.0"/>
                </HBox.margin>
            </Button>
        </HBox>
        <Slider fx:id="redLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="redLightness" GridPane.columnIndex="2"/>
        <Slider fx:id="yellowLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="yellowLightness" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Slider fx:id="greenLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="greenLightness" GridPane.columnIndex="2"
                GridPane.rowIndex="2"/>
        <Slider fx:id="cyanLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="cyanLightness" GridPane.columnIndex="2"
                GridPane.rowIndex="3"/>
        <Slider fx:id="blueLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0"
                showTickMarks="true" snapToTicks="true" styleClass="blueLightness" GridPane.columnIndex="2"
                GridPane.rowIndex="4"/>
        <Slider fx:id="magentaLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0"
                minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="magentaLightness" GridPane.columnIndex="2" GridPane.rowIndex="5"/>
        <Slider fx:id="saturationLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0"
                minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="saturationLightness" GridPane.columnIndex="2" GridPane.rowIndex="6"/>

    </GridPane>

</AnchorPane>
