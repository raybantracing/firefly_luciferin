            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="242.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="10.0" />
        </padding>
        <Label fx:id="redLabel" text="%fxml.colorcorrection.red" wrapText="true" />
        <Label fx:id="yellowLabel" text="%fxml.colorcorrection.yellow" wrapText="true" GridPane.rowIndex="1" />
        <Label fx:id="greenLabel" text="%fxml.colorcorrection.green" wrapText="true" GridPane.rowIndex="2" />
        <Label fx:id="cyanLabel" text="%fxml.colorcorrection.cyan" wrapText="true" GridPane.rowIndex="3" />
        <Label fx:id="blueLabel" text="%fxml.colorcorrection.blue" wrapText="true" GridPane.rowIndex="4" />
        <Label fx:id="magentaLabel" text="%fxml.colorcorrection.magenta" wrapText="true" GridPane.rowIndex="5" />
        <Label fx:id="masterLabel" text="%fxml.colorcorrection.saturation" wrapText="true" GridPane.rowIndex="6" />

        <Slider fx:id="redSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="redSaturation" GridPane.columnIndex="1" />
        <Slider fx:id="yellowSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="yellowSaturation" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Slider fx:id="greenSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="greenSaturation" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Slider fx:id="cyanSaturation" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="cyanSaturation" GridPane.columnIndex="1" GridPane.rowIndex="3" />
