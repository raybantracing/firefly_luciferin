        <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#saveAndClose" text="%fxml.colorcorrection.ok" GridPane.rowIndex="7">
            <GridPane.margin>
                <Insets top="5.0" />
            </GridPane.margin>
        </Button>
        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="7">
            <padding>
                <Insets top="6.0" />
            </padding>
            <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#close" text="%fxml.colorcorrection.cancel">
                <HBox.margin>
                    <Insets />
                </HBox.margin>
            </Button>
            <Button minWidth="60.0" mnemonicParsing="false" onMouseClicked="#reset" text="%fxml.colorcorrection.reset">
                <HBox.margin>
                    <Insets left="7.0" />
                </HBox.margin>
            </Button>
        </HBox>
        <Slider fx:id="redLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="redLightness" GridPane.columnIndex="3" />
        <Slider fx:id="yellowLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="yellowLightness" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Slider fx:id="greenLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="greenLightness" GridPane.columnIndex="3" GridPane.rowIndex="2" />
        <Slider fx:id="cyanLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="cyanLightness" GridPane.columnIndex="3" GridPane.rowIndex="3" />
        <Slider fx:id="blueLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="blueLightness" GridPane.columnIndex="3" GridPane.rowIndex="4" />
        <Slider fx:id="magentaLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="magentaLightness" GridPane.columnIndex="3" GridPane.rowIndex="5" />
        <Slider fx:id="saturationLightness" blockIncrement="0.1" majorTickUnit="0.1" max="1.0" min="-1.0" minorTickCount="0" showTickMarks="true" snapToTicks="true" styleClass="saturationLightness" GridPane.columnIndex="3" GridPane.rowIndex="6" />
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.halignment="CENTER"
              GridPane.rowSpan="7" GridPane.valignment="CENTER">
            <!-- HSL range on the six colors RGB CMY varies from 0° to 360°. Every colors is mapped inside 60°. Decrease this range to 56° on the hue selector for simplicity. -->
            <!-- Some tones can be mapped on two colors when overlaps each other, reducing the range to 56° create no overlapping colors. -->
            <Slider fx:id="hueSlider" disable="true" blockIncrement="2.0" majorTickUnit="2.0" max="28.0" min="-28.0"
                    minorTickCount="0" orientation="VERTICAL" prefHeight="281.0" prefWidth="14.0" showTickMarks="true"
                    snapToTicks="true" styleClass="masterHueTestImage">
                <VBox.margin>
                    <Insets left="20.0"/>
                </VBox.margin>
            </Slider>
        </VBox>

    </GridPane>

</AnchorPane>
